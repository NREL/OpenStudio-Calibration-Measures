  <class_name>ChangeExteriorWallThermalProperties</class_name>
  <display_name>Exterior Wall Thermal Properties Multiplier</display_name>
  <description>Allows calibration of exterior walls by altering the thermal resistance, density, and solar absorptance of the wall constructions independently.</description>
  <modeler_description>Tunes all exterior wall surfaces by providing separate calibration multipliers to thermal resistance, thermal mass, and solar absorptance.</modeler_description>
  <arguments>
    <argument>
      <name>r_value_mult</name>
      <display_name>Exterior wall total R-value multiplier</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>solar_abs_mult</name>
      <display_name>Exterior wall solar absorptance multiplier</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>thermal_mass_mult</name>
      <display_name>Exterior wall thermal mass multiplier</display_name>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
  </arguments>
