      <display_name>CSV file name for the metered data</display_name>
      <description>CSV file name.</description>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>mtr.csv</default_value>
    </argument>
    <argument>
