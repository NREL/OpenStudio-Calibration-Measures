  <class_name>GeneralSpaceTypeCalibrationMeasure</class_name>
  <display_name> General Space Type Calibration Measure</display_name>
  <description>This is a general purpose measure to calibrate space type elements.</description>
  <modeler_description>It will have a single loop through space types. Initially it will be used for calibration of people, infiltration, and outdoor air. It doesn't have to hit the space type json file since it is just adjusting by a multiplier.</modeler_description>
  <arguments>
    <argument>
      <name>multiplier_occ</name>
      <display_name>Occupancy Multiplier</display_name>
      <description>For each space type multiply the default number of people by this value.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>multiplier_infiltration</name>
      <display_name>Infiltration Multiplier</display_name>
      <description>For each space type multiply the default infiltration value by this.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>multiplier_ventilation</name>
      <display_name>Ventilation Multiplier</display_name>
      <description>For each space type multiply the default infiltration value by this.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
  </arguments>
