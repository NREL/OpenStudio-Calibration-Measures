  <class_name>GeneralCalibrationMeasure</class_name>
  <display_name>General Calibration Measure</display_name>
  <description>This is a general purpose measure to calibrate space and space type elements.</description>
  <modeler_description>It will be used for calibration of people, infiltration, and outdoor air.</modeler_description>
  <arguments>
    <argument>
      <name>space_type</name>
      <display_name>Apply the Measure to a Specific Space Type or to the Entire Model.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>*Entire Building*</default_value>
      <choices>
        <choice>
          <value>{a202c075-44b2-4525-b7e9-826a78d1ec0a}</value>
          <display_name>*Entire Building*</display_name>
        </choice>
        <choice>
          <value>0</value>
          <display_name>*None*</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>multiplier_occ</name>
      <display_name>Occupancy Multiplier</display_name>
      <description>For each space type multiply the default number of people by this value.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>multiplier_infiltration</name>
      <display_name>Infiltration Multiplier</display_name>
      <description>For each space type multiply the default infiltration value by this.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
    <argument>
      <name>multiplier_ventilation</name>
      <display_name>Ventilation Multiplier</display_name>
      <description>For each space type multiply the default infiltration value by this.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>1</default_value>
    </argument>
  </arguments>
