  <class_name>GeneralCalibrationMeasure</class_name>
  <display_name>General Calibration Measure</display_name>
  <description>This is a general purpose measure to calibrate space and space type elements.</description>
  <modeler_description>It will be used for calibration of people, infiltration, and outdoor air. User can choose between a SINGLE SpaceType or ALL the SpaceTypes as well as a SINGLE Space or ALL the Spaces.</modeler_description>
  <arguments>
    <argument>
      <name>space_type</name>
      <display_name>Apply the Measure to a SINGLE SpaceType, ALL the SpaceTypes or NONE.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>*All SpaceTypes*</default_value>
      <choices>
        <choice>
          <value>{673e75df-b715-488a-8c0b-198ad9873f6a}</value>
          <display_name>*All SpaceTypes*</display_name>
        </choice>
        <choice>
          <value>0</value>
          <display_name>*None*</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>space</name>
      <display_name>Apply the Measure to a SINGLE Space, ALL the Spaces or NONE.</display_name>
      <type>Choice</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>*All Spaces*</default_value>
      <choices>
        <choice>
          <value>{673e75df-b715-488a-8c0b-198ad9873f6a}</value>
          <display_name>*All Spaces*</display_name>
        </choice>
        <choice>
          <value>0</value>
          <display_name>*None*</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>occ_perc_change</name>
      <display_name>Occupancy Multiplier</display_name>
      <description>Percent Change in the default number of people by this value.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>infil_perc_change</name>
      <display_name>Infiltration Multiplier</display_name>
      <description>Percent Change in the default infiltration value by this.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
    <argument>
      <name>vent_perc_change</name>
      <display_name>Ventilation Multiplier</display_name>
      <description>Percent Change in the default Ventilation value by this.</description>
      <type>Double</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
      <default_value>0</default_value>
    </argument>
  </arguments>
